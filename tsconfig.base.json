{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "declaration": true,
    "resolveJsonModule": true,
    "emitDeclarationOnly": false,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "Node16",
    "target": "ES2022",
    "module": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "stripInternal": true
  }
}
