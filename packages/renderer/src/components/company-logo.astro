---
import { loadSiteData } from "../asset-loader";
import { linkUrl } from "../utils/link-url";
const siteData = await loadSiteData();
---

{
  siteData.logo && (
    <img
      src={linkUrl(siteData.logo.url)}
      alt={siteData.logo.alt}
      width={siteData.logo.width}
      height={siteData.logo.height}
    />
  )
}
