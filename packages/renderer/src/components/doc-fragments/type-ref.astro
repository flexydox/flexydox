---
import { DocTypeRef } from "@flexydox/doc-schema";
import { typeRefToTypeLinkMap } from "../../astro-utils/type-ref-to-type-link-map";
import TypeLink from "./type-link.astro";
const typeRef: DocTypeRef = Astro.props.typeRef;

const typeLinkMap = typeRefToTypeLinkMap(typeRef);
---

{
  typeLinkMap.list.map((typeLink) => (
    <TypeLink typeId={typeLink.typeId} link={typeLink} />
  ))
}

{
  typeLinkMap.none.map((typeLink) => (
    <TypeLink typeId={typeLink.typeId} link={typeLink} />
  ))
}

{typeLinkMap.array.map((typeLink) => " []")}
